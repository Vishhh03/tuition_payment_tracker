{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "vercel_server/app/api/**/*.js",
      "use": "@vercel/node@20.0.0",
      "config": {
        "includeFiles": [
          "vercel_server/app/lib/**",
          "vercel_server/model/**"
        ]
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/vercel_server/app/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}